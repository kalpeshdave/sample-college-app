<%= form_for(@course) do |f| %>

<div>
	<%= f.label :number %><br />
	<%= f.text_field :number %>
</div>

<div>
	<%= f.label :name %><br />
	<%= f.text_field :name %>
</div>

<div>
	<%= f.label :description %><br />
	<%= f.text_field :description %>
</div>

<div>
	<%= f.submit 'Submit' %>
</div>

<% end %>
